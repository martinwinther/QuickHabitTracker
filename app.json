{
  "expo": {
    "name": "Quick Habit Tracker",
    "slug": "quick-habit-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quickhabittracker",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "A beautiful, simple habit tracker focused on building one habit at a time.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.martinsw.QuickHabitTracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "274bfaec-2eab-4af3-a0ea-2b6b945803c9"
      }
    }
  }
}
